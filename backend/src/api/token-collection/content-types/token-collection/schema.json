{
    "kind": "collectionType",
    "collectionName": "token_collections",
    "info": {
        "singularName": "token-collection",
        "pluralName": "token-collections",
        "displayName": "Token Collection",
        "description": "High-level groups like Color, Typography, Spacing"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
